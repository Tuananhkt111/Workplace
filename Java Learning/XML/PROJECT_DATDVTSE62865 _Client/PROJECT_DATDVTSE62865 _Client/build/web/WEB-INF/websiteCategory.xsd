<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.netbeans.org/schema/websiteCategory"
            xmlns="http://xml.netbeans.org/schema/websiteCategory"
            elementFormDefault="qualified">

    <xsd:element name="websiteCategory">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="category" type="CategoryDTO"/>
                <xsd:element name="id" type="xsd:int"/>
                <xsd:element name="name" type="xsd:string"/>
                <xsd:element name="url" type="xsd:string"/>
                <xsd:element name="website" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="CategoryDTO">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:int"/> 
            <xsd:element name="levelAuth" type="xsd:int"/> 
            <xsd:element name="name" type="xsd:string"/> 
            </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
